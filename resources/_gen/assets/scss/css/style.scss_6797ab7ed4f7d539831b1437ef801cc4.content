*,*::after,*::before{-webkit-box-sizing:border-box;-moz-box-sizing:border-box;box-sizing:border-box}html *{-webkit-font-smoothing:antialiased;-moz-osx-font-smoothing:grayscale}body{margin:0;padding:0}.preloader{position:fixed;width:100vw;height:100vh;top:0;left:0;background-color:#2b2c2f;z-index:99999;display:flex;align-items:center;justify-content:center;opacity:1;transition:opacity .5s linear}.preloader.fade-out{opacity:0;transition:opacity .5s linear;display:none}.navbar{-webkit-transition:all ease-out .5s;-moz-transition:all ease-out .5s;-o-transition:all ease-out .5s;transition:all ease-out .5s}.nav-link.active{font-weight:700;-webkit-transition:all ease-out .5s;-moz-transition:all ease-out .5s;-o-transition:all ease-out .5s;transition:all ease-out .5s}.bg-gray{background:rgba(50,50,50,.6)}header#home{overflow:hidden;max-width:100vw;max-height:100wh}header#home #particle{position:relative;overflow:hidden;z-index:-1;background:rgba(50,50,50,.6);background:linear-gradient(180deg,rgba(50,50,50,.6) 0%,rgba(255,255,255,.6) 100%)}header#home .title{justify-content:center;align-items:center;position:absolute;display:flex;height:100%;width:100%;left:0;top:0}@keyframes blink{0%{opacity:1}40%{opacity:1}60%{opacity:0}100%{opacity:0}}.typing-wrapper{display:block;position:relative;text-align:left}.typing-wrapper span.typing-text::before{content:"";display:inline;position:absolute;white-space:nowrap;display:none;left:0;top:0}.typing-wrapper span.typing-text{position:absolute;overflow:hidden;left:0;top:0}.typing-wrapper span.typing-text::after{content:"";position:relative;top:7px;display:inline-block;background-color:#2b2c2f;height:1.25em;width:2px;animation:blink .3s linear infinite alternate}#typing-data>*{display:none}#profile-pic{display:block;min-width:200px;max-width:400px;width:100%;height:auto;margin:auto}.button{text-decoration:none;color:#4d4d4d;font-weight:700;margin-top:15px;display:inline-block;border:2px solid #4d4d4d;padding:7px 10px}.button:hover{background:#6f6f6f;color:#fff!important;border:2px solid #6f6f6f}.timeline{list-style:none;padding:0 0 0 6px;margin-top:40px}.timeline li span,.timeline li p{color:#6f6f6f}ul.timeline li{padding-left:30px;border-left:2px solid #6f6f6f;padding-bottom:10px;position:relative}ul.timeline li:last-child{padding-bottom:0}ul.timeline li::after{position:absolute;top:-1px;left:-11px;right:0;width:20px;height:20px;border-radius:50%;border:2px solid #4d4d4d;background:#fff;content:"";transition:all .7s}ul.timeline li:hover:after{background:#4d4d4d}.timeline-content{position:relative;top:-2px}.skill-bar{margin-top:42px}.skillbar{position:relative;display:flex;margin-bottom:20px;align-items:center;width:100%;background:#fff;border:2px solid #6f6f6f;height:16px;border-radius:22px;transition:.2s linear;transition-property:width,background-color}.skillbar-title{color:#4d4d4d;display:inline-block;margin-bottom:7px}.skill-bar-percent{color:#4d4d4d;display:inline-block;margin-bottom:7px}.skillbar-bar{height:10px;margin:0 2px;width:0;background:#4d4d4d;border-radius:22px}button.button{background-color:initial}.spinner{display:inline-block;position:relative;top:5px;border:4px solid #f3f3f3;border-top:4px solid #4d4d4d;border-radius:50%;width:20px;height:20px;transition:all .2s ease;animation:spin 1s linear infinite}@keyframes spin{0%{transform:rotate(0)}100%{transform:rotate(360deg)}}.footer{padding-top:50px;padding-bottom:70px;position:relative;z-index:2}.footer hr{border:1px solid #fff;margin-bottom:5px}.footer-icons{display:flex;width:fit-content;margin:auto;margin-top:15px}.footer-icons div{display:inline-block}.footer-icons a{text-decoration:none;color:#313131;font-size:17px;margin:0 8px}.footer-icons i{transition:all .3s;background:#fff;padding:7px}.footer-icons i:hover{transform:scale3d(1.5,1.5,1);color:#00d6d6}